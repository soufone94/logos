<script type="text/javascript" src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
<script type="text/javascript" src="//static.s-sfr.fr/sport/resources/js/libs/mustache.min.js"></script>

<script type="text/javascript">
    'use strict';
    (function ($) {
        var category = "sportsextremes",
            hasArticle = true,
            FILTERS_URL = {
                'all': '/api/rest/tracking/published/',
                'dreamteam': '/api/rest/tracking/dreamteam/',
                'simple': '/api/rest/tracking/simple/',
                'media': '/api/rest/tracking/media/'
            },
            TPL_IDS = {
                'SIMPLE_TPL': {'tpl': '#simple-tpl', 'containerSelector': '.article-simple'},
                'HEAD_TPL': {'tpl': '#head-article-tpl', 'containerSelector': '.article-container'},
                'SMALL_TPL': {'tpl': '#small-article-tpl', 'containerSelector': '.thumbnail-list'}
            },
            page = null != null ? null : 0,
            endArticle = false,
            getArticlesUrl = FILTERS_URL['all'],
            $filters = $('.socialAndFilters li'),
            $loader = $('.loader'),
            $noArticles = $('.no-articles');
        
        var renderTemplate = function(listJson, fromScroll){
            var $articleWrapper = $(TPL_IDS['SIMPLE_TPL'].containerSelector),
                tpl = $(TPL_IDS['SIMPLE_TPL'].tpl).html();
                
            for(var i in listJson){
                if (listJson[i].creationDateFormatted != listJson[i].modificationDateFormatted || listJson[i].creationTimeFormatted != listJson[i].modificationTimeFormatted) {
                   listJson[i].isModified = true;
                }
                if (!fromScroll) {
                    if (i == 0) {
                        tpl = $(TPL_IDS['HEAD_TPL'].tpl).html();
                        $articleWrapper = $(TPL_IDS['HEAD_TPL'].containerSelector);
                    } else if (i == 1) {
                        tpl = $(TPL_IDS['SMALL_TPL'].tpl).html();
                        $articleWrapper = $(TPL_IDS['SMALL_TPL'].containerSelector);
                    } else if (i == 4) {
                        tpl = $(TPL_IDS['SIMPLE_TPL'].tpl).html();
                        $articleWrapper = $(TPL_IDS['SIMPLE_TPL'].containerSelector);
                    }
                }
                
                if (!fromScroll && i == 0) {
                    $articleWrapper.prepend(Mustache.render(tpl, listJson[i]));
                } else {
                    $articleWrapper.append(Mustache.render(tpl, listJson[i]));
                }
            }
        };
        
        var getArticles = function(categoryName, numPage, opt){
            var url = ''
                
            if(categoryName != '' && getArticlesUrl != FILTERS_URL['media']){
                url = getArticlesUrl+'category/'+categoryName+'/?page='+numPage
            } else if (getArticlesUrl == FILTERS_URL['media']) {
                url = getArticlesUrl + '?categories=' + categoryName + '&page=' + numPage;
            } else {
                url = getArticlesUrl+'?page='+numPage
            }
            
            $.ajax({
               url : url,
               type : 'GET',
               dataType : 'JSON',
               success : function(resultat, statut){
                   if(resultat != 'undefined' && resultat != ''){
                       renderTemplate(resultat, opt.fromScroll);
                   }else{
                       endArticle = true;
                   }
                   $loader.addClass('hidden');
                   //Nécessaire de masquer la barre qui sépare les articles lorsque article-container est vide
                   if (!opt.fromScroll) {
                       $('.thumbnail-list').removeClass('hidden');
                   }
               },
        
               error : function(resultat, statut, erreur){
                   console.log(erreur);
                   $loader.addClass('hidden');
               }
        
            });
        }
        
        var LinkPageRels = function(page) {
            var beforeVal = $('.page-before').text();
            var afterVal = $('.page-after').text();
            
            if (beforeVal && page) {
                $('head').append('<link rel="prev" href="/'+"sports-extremes"+'/?page='+beforeVal+'">');
            } else if (beforeVal && !page) {
                $('head').append('<link rel="prev" href="/'+"sports-extremes"+'/">');
            }
            if (afterVal) {
                $('head').append('<link rel="next" href="/'+"sports-extremes"+'/?page='+afterVal+'">');
            }
        };
        
        //infinite scroll
        $(window).on('scroll', function(){
            if(!endArticle && $noArticles.hasClass('hidden') && $loader.hasClass('hidden')){
                var windowTopValue = $(window).scrollTop(),
                   endScrollValue = $(document).height() - $(window).height();
                
                if( windowTopValue >= endScrollValue){
                    $loader.removeClass('hidden');
                    getArticles(category, ++page, {fromScroll: true});
                };
            } 
        });
        
        $('.socialAndFilters li').on('click', function() {
            var $selectedFilter = $(this);
            
           if (!$selectedFilter.hasClass('active')) {
                var selectedClassName = $selectedFilter.attr('class');
                getArticlesUrl = FILTERS_URL[selectedClassName];
                $(TPL_IDS['SIMPLE_TPL'].containerSelector).empty();
                $(TPL_IDS['HEAD_TPL'].containerSelector).find('.mea').remove();
                $(TPL_IDS['SMALL_TPL'].containerSelector).empty();
                page = 0;
                $loader.removeClass('hidden');
                $('.thumbnail-list').addClass('hidden'); //Pour cacher la barre lorque article-container est vide
                getArticles(category, page, {fromScroll: false});
           } 
        });
        
        //full size last thumbnail-item on mobile
        if($('.thumbnail-item').last()){
            $('.thumbnail-item').last().removeClass('small-6').addClass('small-12'); 
        } else{
            $('.thumbnail-item').first().removeClass('small-6').addClass('small-12');
        }
        
        //No articles
        if(!hasArticle){
            $('.no-articles').removeClass('hidden');
        }
        
        LinkPageRels(page);
    })(window.$sfr || window.$ || $);
</script>
<script type="text/javascript">
    'use strict';
    (function ($) {
        var $filterList = $('.socialAndFilters .filters > li'),
            $loader = $('.loader');
        
        var toFilter = function(){
           var $el = $(this),
               $allEl = $filterList,
               type = $(this).attr('class').replace('all', '');
               
            $allEl.removeClass('active');
            $el.addClass('active');
        }
        
        $filterList.on('click', toFilter)
    })(window.$sfr || window.$ || $);
</script>  <script>(function($){
 var description = $('.js-meta-description').text();
 if (description) {
     $('meta[name=description]').attr('content', $('.js-meta-description').text().substring(0,170));
 }
 })($sfr)</script>

