<script>
(function ($) {
    $(document).ready(function(){
        
        
        // HEADER NAV
        
        $('.navigation__categories li').on('click', function(e){
            e.stopPropagation();
            if( $(this).hasClass('category--hasCategory') || $(this).hasClass('category--selected') ){
                if( $(this).hasClass('category--selected') ){
                    $('.navigation__categories li').removeClass('category--selected');
                    if( $(this).parents('li').eq(0).hasClass('category--hasCategory') ){
                        $(this).find('ul').hide();
                        $(this).parents('li').eq(0).addClass('category--selected');
                        var newHeight = parseInt( ($('.category--selected > ul').outerHeight() + $('.category--selected > a').outerHeight()), 10);
                    }
                    else {
                        $('.navigation__categories ul').hide();
                        var newHeight = "auto";
                    }
                }
                else {
                    $('.navigation__categories li').removeClass('category--selected');
                    $(this).addClass('category--selected').find(' > ul').show();
                    $(this).parents('li.category--hasCategory > ul').show();
                    var newHeight = parseInt( ($('.category--selected > ul').outerHeight() + $('.category--selected > a').outerHeight()), 10);
                }
                $('.navigation__categories').css("height", newHeight);
            }
            else {
                //console.log('Suivre le lien');
            }
        });
        
        $('.js-viewMore').on('click', function(e){
            e.preventDefault();
            if( $('.navigation').is(':visible') ){
                $('.navigation').slideUp(1);
                $('html, body').removeClass('noScroll');
                $('.item--more').removeClass('open');
            }
            else {
                $('.navigation').slideDown(1);
                $('html, body').addClass('noScroll');
                $('.item--more').addClass('open');
            
                if( $('.navigation__categories li').hasClass('category--selected') ){
                    $('.category--selected').parents('ul').show();
                    var newHeight = parseInt( ($('.category--selected > ul').outerHeight() + $('.category--selected > a').outerHeight()), 10);
                    $('.navigation__categories').css("height", newHeight);
                }
            }
        });
        
    });
})($sfr);
</script>



    


