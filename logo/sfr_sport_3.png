<style>
    *{
        outline: none;
    }
    body{
        background-color: #f8f8f8;
    }
    a{
        text-decoration: none;
        color: initial;
    }
    .row .row{
        margin-top: 0;
        margin-bottom: 0;
    }
    .hidden{
        display: none;
    }
    .loader{
        background: #FFF;
    }
    .loader img{
        width: 50px;
        height: 50px;
        margin: 0 auto;
    }
    .sport-list{
        max-width: 1285px;
    }
    
    /* ARTICLE FILTERS */
    .article-filter {
        position: relative;
        z-index: 1;
        font-size: 0;
        margin: 0;
        padding: 0 15px;
        background: #FFF;
        display: inline-block;
        vertical-align: top;
        height: 24px;
    }
    .article-filter__item{
        display: inline-block;
        font-size: 14px;
        font-family: "SFR-Regular", Sans-Serif;
        color: #666;
        vertical-align: top;
        height: 100%;
        margin-left: 26px;
        cursor: pointer;
    }
    .article-filter__item:last-child{
        margin-right: 0;
    }
    .article-filter__item:before {
        content: "";
        display: inline-block;
        width: 10px;
        height: 10px;
        border-radius: 50%;
        margin-right: 10px;
        background-color: #adadad;
    }
    .article-filter .active {
        color: #e2001a;
    }
    .article-filter .active:before {
        background-color: #e2001a;
    }
    
    /* ARTICLE */
    .title--1{
        font-size: 34px;
        margin: 20px 0 15px 0;
    }
    .title-th{
        font-family: "SFR-Bold", Sans-Serif;
        font-size: 22px;
        color: #000;
        margin: 15px 0 15px 0;
        padding: 0;
    }
    
    .post-infos .fa{
        position: relative;
        font-size: 20px;
        vertical-align: top;
        margin-right: 0;
        color: #a0a0a0;
    }
    .post-infos .sep, .author{
        padding: 0 5px;
    }
    .post-infos .sep{
        border-right: solid 1px #a0a0a0;
    }
    .date{
        line-height: initial;
    }
    p.paragraphe{
        margin-top: 10px;
        margin-bottom: 0;
    }
    .thumbnail-list{
        padding: 10px;
        margin-bottom: 10px;
        background: #FFF;
    }
    .thumbnail-img{
        position: relative;
        width: 100%;
        height: 170px;
        /*background-position: center center;*/
        background-size: cover;
    }
    .thumbnail-img.small {
        height: 90px;
    }
    .thumbnail-content-info{
        display: none;
    }
    .article-item{
        margin-bottom: 10px;
        background: #FFF;
    }
    .result{
        padding-top: 15px;
        padding-bottom: 15px;
        margin-top: 5px;
        border-top: solid 1px #e3e3e3;
        border-bottom: solid 1px #e3e3e3;
        font-family: 'ClearSans-Light', 'SFR-Bold', Sans-Serif;
        font-size: 12px;
    }
    .result-score{
        font-family: 'ClearSans-Bold', 'SFR-Bold', Sans-Serif;
        font-size: 14px;
        text-align: center;
    }
    .result-teamName.right{
        text-align: right;
    }
    .team-flag{
        width: initial;
        vertical-align: middle;
    }
    .no-articles{
        padding: 40px 0;
        text-align: center;
        font-size: 25px;
        font-family: 'SFR-Bold', Arial, sans-serif;
        text-transform: uppercase;
    }
    /* SIDEBAR */
    .sidebar__block:first-child {
        margin-top: 0;
    }
    @media screen and (min-width:768px){
        .article-filter {
            float: right;
        }
        .article-filter__item{
            margin-right: 30px;
        }
        .title--1{
            margin: 0 0 15px 0;
        }
        .article-item.mea .title--1{
            margin: 15px 0;
        }
        .article-item.mea .title--1, .title--1.title-th{
            margin: 15px 0;
        }
        .article-container{
            padding: 20px;
            background: #FFF;
        }
        .article-item{
            margin-bottom: 0;
            padding: 40px 0;
            border-bottom: solid 1px #e3e3e3;
            background: transparent;
        }
        .article-item:first-child{
            padding:0;
            border-bottom: none;
        }
        .article-item:last-child{
            border-bottom: none;
        }
        .article-item-details{
            padding-left: 15px;
        }
        .thumbnail-list{
            padding: 10px 0 40px 0;
            border-bottom: solid 1px #e3e3e3;
        }
        .thumbnail-item:first-child{
            padding-left: 0; 
        }
        .thumbnail-item:last-child{
            padding-right: 0; 
        }
        .thumbnail-img{
            height: 213px;
        }
        .article-item.mea .thumbnail-img{
            height: 370px;
        }
        .thumbnail-img.small{
            height: 180px;
        }
        .thumbnail-content-info{
            display: none;
        }
        .article-simple .article-item:first-child{
            padding: 40px 0;
            border-bottom: solid 1px #e3e3e3;
        }
        .no-articles{
            font-size: 35px;
        }
        .article-item.mea .post-infos{
           margin-bottom: 10px;
        }
    }
    @media screen and (max-width:767px){
        .article-item {
            padding: 10px;
        }
        .article-item:first-child {
            margin-bottom: 0;
        }
        .title--1 {
            font-size: 18px;
            margin-top: 15px;
        }
        .thumbnail-item:last-child{
            padding: 0;
        }
        .thumbnail-item:last-child .thumbnail-img.small{
            height: 170px;
        }
        .thumbnail-item:last-child .thumbnail-content-info{
            display: block;
        }
    }
    @media screen and (max-width:1024px){
        [class*="column"] + [class*="column"]:last-child {
            float: left;
        }
        .date .fa {
            display: inline-block;
        }
    }
</style>
